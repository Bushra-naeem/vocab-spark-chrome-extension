{
    "manifest_version": 3,
    "name": "Vocabulary Spark",
    "version": "1.0",
    "description": "Get a new word with its meaning every time!",
    "permissions": ["storage"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icon.png",
            "48": "icon.png",
            "128": "icon.png"
        }
    },
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "128": "icon.png"
    }
}
